% feta-nummer4.mf
% part of LilyPond's pretty-but-neat music font

input autometric;

fet_beginfont("feta-nummer", 4);

mode_setup;

test:=0;
input feta-macros;
input feta-params;

height#:=designsize;

fatten:=1.15;
widen:=1.05;
flaren:=0.9;
thick#:=7/30height#*fatten;
thin#:=thick#/4*fatten;
hair#:=blot_diameter#;
kuulleke#:=1/8thick#;

message "thin:"&decimal(thin#);
message "blot:"&decimal(blot_diameter#);

input feta-nummer;

fet_endfont("feta-nummer");

end.

